@using System.Security.Claims;
@using NimbRepository.Model;
@{
    ClaimsIdentity identity = ViewBag.Identity; 
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@TempData["Check"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Nimb.styles.css" asp-append-version="true" />

    <link href="/css/Seller/indexStyle.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.2/css/jquery.dataTables.min.css">
    <script src="https://kit.fontawesome.com/363a110c1e.js" crossorigin="anonymous"></script>

</head>
<body>
    <header>
        @if (TempData["Check"] != null)
        {
            if (TempData["Check"].ToString() != "")
            {

                    <partial name="_Layoutnav" />
            }
        }    
          
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
