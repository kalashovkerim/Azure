<link href="/css/Seller/indexStyle.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.2/css/jquery.dataTables.min.css">

<form class="buttons">
    <button asp-controller="Seller" asp-action="Goods" class="firstButton" value="Goods" type="submit"><p>Goods</p></button>
    <button asp-controller="Seller" asp-action="RegisterUser" class="secondButton" value="Add client" type="submit"><p>Add client</p></button>
    <button asp-controller="Seller" asp-action="PlaceAnOrder" class="fourthButton" value="Place an order" type="submit"><p>Place an order</p></button>
</form>

<div class="bg">
    <div class="table">
        <div class="dataTables_wrapper">
            <table id="table_id" class="display nowrap dataTable dtr-inline collapsed" style="width:100%; position: relative;">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Last name</th>
                        <th>Patronymic name</th>
                        <th>Number</th>
                        <th>EmailAddress</th>
                        <th style="text-align: center;">Edit or Delete</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>


@section Scripts{
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script defer type="text/javascript" src="https://cdn.datatables.net/1.13.2/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="/js/seller.js"></script>
}